###  http://purl.org/cyber/cve#CVE
:CVE rdf:type owl:Class ;
     rdfs:subClassOf :CVEHandling ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :has_references ;
                       owl:someValuesFrom :References
